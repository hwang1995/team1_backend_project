<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
                        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<!-- DTO를 적어줍시다. -->
		<typeAlias type="com.team1.healthcare.dto.DiagnosisDTO"
			alias="diagnosis" />
		<typeAlias
			type="com.team1.healthcare.dto.DiagnosticInspectionsDTO"
			alias="diagnosticInspections" />
		<typeAlias
			type="com.team1.healthcare.dto.DiagnosticTestRecordsDTO"
			alias="diagnosticTestRecords" />
		<typeAlias
			type="com.team1.healthcare.dto.DiagnosticTestsDTO"
			alias="diagnosticTests" />
		<typeAlias type="com.team1.healthcare.dto.HospitalsDTO"
			alias="hospitals" />
		<typeAlias
			type="com.team1.healthcare.dto.MedicineRecordsDTO"
			alias="medicineRecords" />
		<typeAlias type="com.team1.healthcare.dto.MedicinesDTO"
			alias="medicines" />
		<typeAlias type="com.team1.healthcare.dto.NoticesDTO"
			alias="notices" />
		<typeAlias type="com.team1.healthcare.dto.PatientsDTO"
			alias="patients" />
		<typeAlias type="com.team1.healthcare.dto.VitalRecordsDTO"
			alias="vitalRecords" />

		<typeAlias type="com.team1.healthcare.dto.NoticeCommentsDTO"
			alias="noticeComments" />

		<typeAlias type="com.team1.healthcare.dto.TodosDTO"
			alias="todos" />

		<typeAlias type="com.team1.healthcare.dto.MembersDTO"
			alias="members" />

		<!-- VO를 적어줍시다. -->
		<typeAlias type="com.team1.healthcare.vo.auth.LoginVO"
			alias="loginInfo" />

		<typeAlias
			type="com.team1.healthcare.vo.diagnosis.RegistDiagnosisResultVO"
			alias="registDiagnosisResult" />

		<typeAlias
			type="com.team1.healthcare.vo.diagnosis.MedicineResultVO"
			alias="medicineResult" />

		<typeAlias
			type="com.team1.healthcare.vo.diagnosis.VitalResultVO"
			alias="vitalResult" />

		<typeAlias
			type="com.team1.healthcare.vo.common.DateWithMemberVO"
			alias="dateWithMember" />

		<typeAlias
			type="com.team1.healthcare.vo.common.DateWithHospitalCode"
			alias="dateWithHospitalCode" />

		<typeAlias
			type="com.team1.healthcare.vo.diagnostic.DiagnosticTestResultVO"
			alias="diagnosticTestResult" />
			
	</typeAliases>

</configuration>